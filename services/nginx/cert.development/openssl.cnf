[req]
distinguished_name = req_distinguished_name
req_extensions     = v3_req
x509_extensions    = v3_ca
prompt = no

[req_distinguished_name]
countryName            = PL
stateOrProvinceName    = XX
localityName           = XX
postalCode             = XX
streetAddress          = XX
organizationName       = Enraged
organizationalUnitName = Enraged
commonName             = enraged.local
emailAddress           = contact@enraged.local

[v3_ca]
subjectAltName = @alt_names

[v3_req]
subjectAltName = @alt_names

[alt_names]
DNS.1 = *.enraged.local
DNS.2 = enraged.local
